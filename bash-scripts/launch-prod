sudo usermod -a -G docker ec2-user
sudo service docker start

docker kill Website-Prod
docker rm Website-Prod

docker kill Website-Dev
docker rm Website-Dev

docker build https://github.com/ModernMAK/Personal-Website.git#production -t personal-website-prod

docker run --name Website-Prod -v "/etc/letsencrypt:/etc/letsencrypt" -v "/var/lib/letsencrypt:/var/lib/letsencrypt" -p 80:8080 -p 443:8080 personal-website-prod